{
  "name": "@emonkak/di",
  "version": "1.0.0",
  "description": "A lightweight dependency injector",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/emonkak/di.js.git"
  },
  "scripts": {
    "test": "mocha",
    "coverage": "mocha --require blanket --reporter html-cov > coverage.html",
    "coveralls": "mocha --require blanket --reporter mocha-lcov-reporter | coveralls"
  },
  "author": "Shota Nozaki <emonkak@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "blanket": "^1.1.7",
    "coveralls": "^2.11.3",
    "expect.js": "^0.3.1",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  }
}
